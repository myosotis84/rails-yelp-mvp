<div class="content">
  <h1>Add a new restaurant</h1>
  <div class="form">
    <%= simple_form_for @restaurant do |f| %>
      <%= f.input :name %>
      <%= f.input :category %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.submit "Create restaurant", class: "btn btn-primary" %>
    <% end %>
  </div>
  <p><%= link_to "Back to all restaurants", restaurants_path %></p>
</div>
